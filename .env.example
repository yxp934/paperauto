# Copy this file to .env and fill in your own keys. DO NOT COMMIT .env

# Hugging Face API
HUGGINGFACE_API_KEY=

# LLM API (Google Gemini)
LLM_API_URL=https://generativelanguage.googleapis.com/v1beta/models/gemini-2.5-pro:generateContent
LLM_API_KEY=
LLM_MODEL=gemini-2.5-pro

# Image Generation API (ModelScope Qwen-image)
IMAGE_API_URL=https://api-inference.modelscope.cn/v1/images/generations
IMAGE_API_KEY=
IMAGE_MODEL=MusePublic/Qwen-image

# Text-to-Speech API (DashScope)
TTS_API_URL=https://dashscope.aliyuncs.com/compatible-mode/v1/chat/completions
TTS_API_KEY=
DASHSCOPE_API_KEY=

# Output config
OUTPUT_DIR=./output
TEMP_DIR=./temp

# Logging
LOG_LEVEL=INFO
BILIBILI_ENABLED=true

